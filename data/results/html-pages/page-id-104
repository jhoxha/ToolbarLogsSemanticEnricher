	<script type="text/javascript">
		// Bootstrap the timing code, see eb.timing.js
		function setCookie(name, value, options) {
		    var c = name + '=' + escape(value);
		    for (key in options) {
		    	c += ';' + key + '=' + options[key];
		    }
		    document.cookie = c;
		}

		var tStart = new Date().getTime(),
			tCookieSet,
			cookieOpts = {
				expires: new Date(tStart + (600 * 1000)).toUTCString(), // Expires in 10 minutes
				path: '/',
				domain: 'eventbrite.com'
			};

		setCookie('eb_timing_t_start', tStart.toString(), cookieOpts);
		// Spell referrer correctly here because the DOM spells it correctly
		setCookie('eb_timing_referrer', document.referrer.replace(/#.*/, ''), cookieOpts);

		tCookieSet = new Date().getTime();
		if (tCookieSet - tStart > 50) {
			// User probably has cookie prompting turned on so we
			// won't get accurate measurements.  Delete the cookies.
			cookieOpts = { path: '/', domain: 'eventbrite.com', expires: -1 };
		    setCookie('eb_timing_t_start', null, cookieOpts);
		    setCookie('eb_timing_referrer', null, cookieOpts);
		}
	</script>
