        window.location.hash = '';
        var eventTrackingRouter = new EB.EventTracking.Router({
            FB: FB,
            ebServer: 'http://www.eventbrite.com',
            eventData: {"start_date_month_short": "May", "address_short": "1904 Chestnut, 267-495-7012, Philadelphia", "name": "Clubhouse at Industry XIX Membership Request Form | Follow us @industryxix", "start_date_day_of_the_week": "Fri", "url": "http://myclubhouse-esv.eventbrite.com", "start_date_formatted": "May 4, 2012", "repeats": false, "id": 3456956851, "categories": ["Social Events", "Networking"], "start_date": 1336190400000},
            saveOnInit: EB.EventTracking.userClickedSave,
            useLocalStorage: false,
            fetchCount: true
        });
        Backbone.history.start();
        EB.EventTracking.Analytics.init(
            eventTrackingRouter,
            {
                'eventId': 3456956851,
                'internalReferrer': '7'
            }
        );
    });

            });
        }
    })(window.jQuery, window.EB);
