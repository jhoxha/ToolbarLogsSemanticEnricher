       function displaySpecial(desc){
         var specialArray = new Array('','%uE000','%uF8FF','%uE001','%uE002','%uE003','%uE004','%uE005','%uE006','%uE007','%uE008','%uE009','%uF000','%uF001','%uF002','%uF8FE');
         var descArray = desc.split('');
         var temp ='';
         for(var i=0;i<descArray.length;i++) {
           temp = escape(descArray[i]);
             if(temp.length==3){
               temp = temp.substring(1,3);
               if(temp>0 && temp<=19) {
                 alert("Please remove the special character "+descArray[i]+" from event description");
               }
             }
             if(specialArray.indexOf(temp)>0){
                 alert("Please remove the special character "+descArray[i]+" from event description");
             }
         }
       }
    </script>

    
    
