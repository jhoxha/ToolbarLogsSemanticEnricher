                      <h1 id="performer-title" itemprop="name" class="n fn">Who's Bad Tour Dates</h1>
                      
                      
                        <h2 id="performer-short-bio">Pop / R&B / Rock</h2>
                      
                      
                      <div class="item-sharer">
  <ul>
            <li class="fb first">
      
      <fb:like href="http://eventful.com/performers/whos-bad-/P0-001-000197845-1" show_faces="no" layout="button_count"  send="true"></fb:like>
      
