            eventData: {"start_date_month_short": "Jul", "address_short": "2 POYDRAS ST, New Orleans, LA", "name": "ESSENCE MUSIC FESTIVAL PARTY BUS TRIP 2013 WITH CHA-CHA", "start_date_day_of_the_week": "Fri", "url": "http://essencemusic2013-esv.eventbrite.com", "start_date_formatted": "Jul 5, 2013 9:00 AM EST", "repeats": false, "id": 3908662916, "categories": ["Travel", "Festivals"], "start_date": 1373040000000},
            saveOnInit: EB.EventTracking.userClickedSave,
            useLocalStorage: false,
