  {{#each results}}
    {{#unless sponsored}}<li class="cat-header cat-header-{{this.type}}">{{this.type}}</li>{{/unless}}
    <li class="{{#if sponsored}}sponsored{{/if}}" data-type="{{this.type}}" {{#if this.spid}}data-spid="{{this.spid}}"{{/if}} {{#if this.svid}}data-svid="{{this.svid}}"{{/if}} {{#if this.smid}}data-smid="{{this.smid}}"{{/if}} {{#if this.name_squish}}data-name-squish="{{this.name_squish}}"{{/if}} {{#if this.location_id}}data-location-id="{{this.location_id}}" data-location-type="{{this.location_type}}"{{/if}} {{#if this.cta_link}}data-cta-link="{{this.cta_link}}"{{/if}}>
      {{this.name}}
      <p>{{itemDescription this}}</p>
      {{#if sponsored}}
        <span class="is-sponsored">sponsored</span>
        <span class="view-schedule">View Full Schedule</span>
      {{/if}}
    </li>
  {{/each}}
</script>

